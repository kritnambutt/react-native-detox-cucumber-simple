{
  "name": "awesome-ts-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "e2e:iphone-build": "detox build -c iphone",
    "e2e:ipad-build": "detox build -c iphone",
    "e2e:android-build": "detox build -c android",
    "e2e:iphone-test": "DETOX_CONFIGURATION=iphone node_modules/.bin/cucumber-js -f json:./e2e/report-iphone/cucumber_report.json --profile iosIphone --require-module @babel/register ./e2e/features --publish",
    "e2e:iphone-test:focus": "DETOX_CONFIGURATION=iphone node_modules/.bin/cucumber-js -f json:./e2e/report-iphone/cucumber_report.json --profile iosIphone --require-module @babel/register ./e2e/features --publish --tags @focus",
    "e2e:iphone-ci": "npm run e2e:iphone-build && npm run e2e:iphone-test -- --cleanup",
    "iphone": "NODE_ENV=PLATFORM=iphone TEST_ENV=local cucumber-js --profile iosIphone --publish",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "cypress": "^10.11.0",
    "jest-cucumber": "^3.0.1",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "react": "18.1.0",
    "react-native": "0.70.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@cucumber/cucumber": "^8.7.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "detox": "^19.12.6",
    "eslint": "^7.32.0",
    "jest": ">=27.2.5",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
